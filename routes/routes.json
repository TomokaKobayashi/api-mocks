{
  "prefix": [
    "/prefix00(/[a-z]{4})?",
    "/prefix01(/[a-z]{4}/)?",
    "/prefix02"
  ],
  "endpoints": [
    {
      "pattern": "/aaa/bbb",
      "method": "GET",
      "matches": [
        {
          "metadata": "responses/response_a/define.json",
          "conditions": "data.AAA===\"BBB\""
        },
        {
          "metadata": "responses/response_b/define.json",
          "conditions": "data.CCC[0]===\"DDD\""
        }
      ]
    },
    {
      "pattern": "/ccc/ddd",
      "method": "GET",
      "matches": [
        {
          "metadata": "responses/response_a/define.json"
        }
      ]
    },
    {
      "pattern": "/ddd/eee",
      "method": "POST",
      "matches": [
        {
          "metadata": "responses/response_a/define.json"
        }
      ]
    },
    {
      "pattern": "/fff/ggg/:name/:type",
      "method": "POST",
      "matches": [
        {
          "metadata": "responses/response_a/define.json"
        }
      ]
    },
    {
      "pattern": "/image/:name",
      "method": "GET",
      "matches": [
        {
          "metadata": "responses/response_c/define.json"
        }
      ]
    }
  ]
}